<h1>Dashboard</h1>

<%= form_for @dashboard.new_text_applause do |form| %>
	<%= form.text_field :body, placeholder: 'Type your applause here...' %>
	<%= form.submit 'Applause!' %>
<% end %>

<%= form_for @dashboard.new_photo_applause do |form| %>
	<%= form.file_field :image %>
	<%= form.submit 'Applause!' %>
<% end %>

<%= render @dashboard.timeline %>